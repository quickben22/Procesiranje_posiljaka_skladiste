             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Procesiranje_posiljaka_skladiste.Moduli"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="370,40,0,0" TextWrapping="Wrap" FontSize="24" Text="ULAZ" VerticalAlignment="Top" RenderTransformOrigin="0.061,-0.406"/>

